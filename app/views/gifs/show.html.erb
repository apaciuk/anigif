<div class="page-header">
  <%= link_to gifs_path, class: 'btn btn-default' do %>
    All Gifs
  <% end %>
  <%= link_to edit_gif_path(@gif), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show gif</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @gif.user %></dd>
  <dt>Image data:</dt>
  <dd><%= @gif.image_data %></dd>
</dl>
